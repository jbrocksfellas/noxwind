{
  "name": "noxwind",
  "version": "1.0.2",
  "description": "A CLI tool to setup Full-Stack application faster",
  "exports": "./bin/index.js",
  "type": "module",
  "bin": {
    "noxwind": "bin/index.js"
  },
  "license": "MIT",
  "types": "./bin/index.d.ts",
  "scripts": {
    "dev": "concurrently \"yarn tsc --watch\" \"nodemon bin/index.js\"",
    "start": "node bin/index.js",
    "build": "yarn tsc",
    "prepare": "yarn run build"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.3",
    "@types/node": "^18.15.11",
    "@types/shelljs": "^0.8.11",
    "@types/yargs": "^17.0.24",
    "concurrently": "^8.0.1",
    "nodemon": "^2.0.22",
    "typescript": "^5.0.3"
  },
  "dependencies": {
    "chalk": "4.1.2",
    "inquirer": "8.0.0",
    "shelljs": "^0.8.5",
    "yargs": "^17.7.1"
  },
  "author": "Gaurav Singh <jbrocksfellas@gmail.com>",
  "keywords": [
    "javascript",
    "typescript",
    "noxwind",
    "command-line",
    "full-stack",
    "cli",
    "nodejs",
    "prompt",
    "react",
    "nextjs",
    "angular",
    "vanilla"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jbrocksfellas/noxwind.git"
  },
  "bugs": {
    "url": "https://github.com/jbrocksfellas/noxwind/issues"
  },
  "homepage": "https://github.com/jbrocksfellas/noxwind#readme",
  "engines": {
    "node": ">=14.16"
  },
  "files": [
    "./bin/**/*"
  ]
}
